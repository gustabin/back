            xmlns:gen_ns="http://ws.oobgen.csd.rsa.com">
	<xsd:import schemaLocation="OOBGen.xsd" namespace="http://ws.oobgen.csd.rsa.com"/>
    <xsd:complexType name="OOBSMSAuthenticationRequest">
        <xsd:complexContent>
            <xsd:extension base="gen_ns:OOBGenAuthenticationRequest">
                <xsd:annotation>
                    <xsd:documentation>This type defines OOBSMS Authentiaction Request</xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                	<xsd:element name="otp" minOccurs="0" type="xsd:string" />
                </xsd:sequence>                	
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="OOBSMSAuthenticationResponse">
        <xsd:complexContent>
            <xsd:extension base="gen_ns:OOBGenAuthenticationResponse">
                <xsd:annotation>
                    <xsd:documentation>This type defines OOBSMS Authentiaction Response</xsd:documentation>
                </xsd:annotation>
                <xsd:sequence/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
